<% content_for :page_title do %>
  <%= Spree.t(:new_supplier) %>
<% end %>

<div data-hook="admin_supplier_new_form_header">
  <%= render :partial => 'spree/admin/shared/error_messages', :locals => { :target => @supplier } %>
</div>

<div data-hook="admin_supplier_new_form">
  <%= form_for [:admin, @supplier], url: spree.admin_suppliers_url, method: :post do |f| %>
    <%= render :partial => 'form', :locals => { :f => f } %>

    <div data-hook="admin_supplier_new_form_buttons">
      <%= render :partial => 'spree/admin/shared/new_resource_links' %>
    </div>
  <% end %>
</div>
